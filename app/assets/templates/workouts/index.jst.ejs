<% workouts.forEach(function (workout) { %>
	<div class="workout" data-id="<%= workout.id %>">
		<p>Date: <%= workout.get('logged_date').substr(0, 10) %></p>

		<% workout.activities.forEach(function (activity) { %>
		<div class="activity" data-id="<%= activity.id %>">
			<p><%= activity.activity_base.get('name') %></p>
			<% activity.activity_sets.forEach(function (set) { %>
				<div class="set offset1" data-id="<%= set.id %>">
				Set: <%= set.escape('set_number') %> | <%= set.describe() %>
				</div>
			<% }) %>
			
		</div>
		<% }) %>
	</div>
<% }); %>